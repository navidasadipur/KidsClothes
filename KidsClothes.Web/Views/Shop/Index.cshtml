                <script>
        var selectedGroupId = @Model.SelectedGroupId;

        function GetGrid(page, groupId) {
            $("#loading").show();
            var pageNumber = 1;
            if (page != null) {
                pageNumber = page;
            }
            var sort = $("#sort").find(":selected").val();
            var take = $("#take").find(":selected").val();
            var categoryId = groupId !== undefined ? groupId : null;
            categoryId = categoryId === null ? selectedGroupId : categoryId;
            var searchString =  '@ViewBag.SearchString';
            var groups = [];
            var brands = [];
            var sizes = [];
            var colors = [];
            var priceFrom = null;
            var priceTo = null;
            var selectedPriceRange = $("span#filter-price-range").text().replaceAll("تومان", "");
            if (selectedPriceRange != undefined) {
                var priceRangeArr = selectedPriceRange.split('-');

                if (parseInt(priceRangeArr[0]) > 0) {
                    priceFrom = parseInt(priceRangeArr[0]);
                }
                if (parseInt(priceRangeArr[1]) > 0) {
                    priceTo = parseInt(priceRangeArr[1]);
                }
            }

            $("input[name='groups']").map(function () {
                if ($(this).prop("checked") === true) {
                    groups.push(parseInt($(this).val()));
                }
            });

            $("input[name='brands']").map(function() {
                if ($(this).prop("checked") === true) {
                    brands.push(parseInt($(this).val()));
                }
            });

            $("input[name='sizes']").map(function () {
                if ($(this).prop("checked") === true) {
                    sizes.push(parseInt($(this).val()));
                }
            });

            $("input[name='colors']").map(function () {
                if ($(this).prop("checked") === true) {
                    colors.push(parseInt($(this).val()));
                }
            });

            var subFeatures = [];
            $("[name='subFeatures']").map(function() {
                if ($(this).prop("checked") === true) {
                    subFeatures.push(parseInt($(this).val()));
                }
            });

            var grid = {
                categoryId: categoryId,
                searchString: searchString,
                priceFrom: priceFrom,
                priceTo: priceTo,
                groups: groups.join('-'),
                brands: brands.join('-'),
                sizes: sizes.join('-'),
                colors: colors.join('-'),
                subFeatures: subFeatures.join('-'),
                pageNumber: pageNumber,
                take: take,
                sort: sort,
                BrandIds: brandIds,
                GroupIds: groupIds,
                ProductIds: productIds,
            }
            $.ajax({
                type: 'GET',
                url: '/ProductsGrid',
                data: grid,
                success: function(data) {
                    $("#grid").html(data);
                },
                error: function(data) {
                    console.log("error");
                },
                complete: function() {
                    $("#loading").hide();
                }
            });
        }

        $(document).ready(function () {

            //Get all buts Of a tag of colors
            var allColorAtagBtns = $(".filter-colors a");

            //Get all checkBoxes of colors
            var allCheckBoxBtns = $(".filter-colors input")

            // Loop through the a tag buttons and add the selected class to the current/clicked button and checked uncisible checkbox
            for (var i = 0; i < allColorAtagBtns.length; i++) {
                allColorAtagBtns[i].addEventListener("click", function () {

                    $(this).toggleClass('selected');

                    var colorId = $(this).attr('id');

                    var checkBoxe = $('.filter-colors input#' + colorId);

                    if (checkBoxe.prop("checked") == true)
                        checkBoxe.prop("checked", false);
                    else
                        checkBoxe.prop("checked", true);
                });
            }



            // Loop through the a checkboxes buttons and add the selected class to the a tag buttons and checked uncisible checkbox
            for (var i = 0; i < allCheckBoxBtns.length; i++) {

                var checkBoxeId = $(allCheckBoxBtns[i]).attr('id')

                var colorInput = $('.filter-colors #' + checkBoxeId);

                if ($(allCheckBoxBtns[i]).prop("checked") == true) {
                    colorInput.toggleClass('selected');
                }
            }



            // Slider For category pages / filter price
            if (typeof noUiSlider === 'object') {
                var priceSlider = document.getElementById('price-slider');

                // Check if #price-slider elem is exists if not return
                // to prevent error logs
                if (priceSlider == null) return;

                noUiSlider.create(priceSlider, {
                    start: [minPrice, maxPrice],
                    connect: true,
                    step: 1000,
                    margin: 2000,
                    range: {
                        'min': minPrice,
                        'max': maxPrice
                    },
                    tooltips: true,
                    format: wNumb({
                        decimals: 0,
                        suffix: 'تومان'
                    })
                });

                // Update Price Range
                priceSlider.noUiSlider.on('update', function (values, handle) {
                    $('#filter-price-range').text(values.join(' - '));
                });
